class Solution(object):  # Define a class named Solution
    def duplicateEvenNums(self, nums):
        result = []

        for num in nums:
          result.append(num)

          if num%2 == 0:
            result.append(num)
            result.append(num)
      

        return result

test = [ 1, 2, 1, 8]

print(Solution().duplicateEvenNums(test))

# O(N) TIME AND SPACE
